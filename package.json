  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/chart.js": "^2.9.19",
    "@types/mapbox-gl": "^1.9.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "sass": "^1.26.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
