{
    "name": "equalstreetnames/equalstreetnames",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.83",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": ""
        }
    },
    "scripts": {
        "analyze": "phpstan analyze",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "geojson": "php scripts/geojson.php",
        "overpass-relation": "php scripts/overpass-relation.php",
        "overpass-way": "php scripts/overpass-way.php",
        "statistics": "php scripts/statistics.php",
        "update-data": [
            "Composer\\Config::disableProcessTimeout",
            "@overpass-relation",
            "@overpass-way",
            "@wikidata",
            "@geojson",
            "@statistics"
        ],
        "wikidata": "php scripts/wikidata.php"
    }
}
